<?xml version="1.0"?>
<!-- this xml file defines the file system skins folder and the layer in Plone. -->
<object name="portal_skins">

  <object name="PloneShellConsole" meta_type="Filesystem Directory View"
          directory="Products.PloneShellConsole:skins/PloneShellConsole"/>

  <skin-path name="*">
    <layer name="PloneShellConsole" insert-after="custom"/>
  </skin-path>
</object>
