<?xml version="1.0"?>
<!-- this xml file defines the file system skins folder and the layer in Plone. -->
<object name="portal_skins">

  <object name="XPointContactManagement" meta_type="Filesystem Directory View"
          directory="Products.XPointContactManagement:skins/XPointContactManagement"/>

  <skin-path name="*">
    <layer name="XPointContactManagement" insert-after="custom"/>
  </skin-path>
</object>
