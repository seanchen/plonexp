<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
        default_skin="iScorpio Themes Redmaple">

  <!-- Objects for skin redmaple -->
  <object name="iscorpio_redmaple_images" meta_type="Filesystem Directory View"
          directory="iscorpio.themes.redmaple:skins/iscorpio_redmaple_images"/>
  <object name="iscorpio_redmaple_styles" meta_type="Filesystem Directory View"
          directory="iscorpio.themes.redmaple:skins/iscorpio_redmaple_styles"/>
  <object name="iscorpio_frog_templates" meta_type="Filesystem Directory View"
          directory="iscorpio.themes.redmaple:skins/iscorpio_redmaple_templates"/>

  <skin-path name="iScorpio Themes Redmaple" based-on="Plone Default">
    <layer name="iscorpio_redmaple_images" insert-after="custom"/>
    <layer name="iscorpio_redmaple_styles" insert-after="iscorpio_redmaple_images"/>
    <layer name="iscorpio_redmaple_templates" insert-after="iscorpio_redmaple_styles"/>
  </skin-path>

</object>
